        "libs/backend/firebase/feature/src/index.ts"
      ],
      "@booking/backend-users-data-access": [
        "libs/backend/users/data-access/src/index.ts"
      ],
      "@booking/backend-users-feature": [
        "libs/backend/users/feature/src/index.ts"
      ],

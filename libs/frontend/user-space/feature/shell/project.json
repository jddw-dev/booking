    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/frontend/user-space/feature/shell/**/*.ts",
          "libs/frontend/user-space/feature/shell/**/*.html"
        ]
      }
